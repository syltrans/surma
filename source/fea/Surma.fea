} DoUkarTrls;

#  Special ukar for glyphs based on Do
lookup DoUkar {
  lookupflag 0 ;
    sub @TakesDoUkar uniA825' by uniA825.Do ;
} DoUkar;

lookup LeftUkar {
  lookupflag 0 ;
