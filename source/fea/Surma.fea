#**********************************
#  Features
#**********************************

feature blwf {  # Below-Base Forms
    script DFLT;  # Default
        language dflt;  # Default
            lookup DoUkarTrls;
            lookup DoUkar;
            lookup LeftUkar;
    script sylo;  # Syloti Nagri
        language dflt;  # Default
            lookup DoUkarTrls;
            lookup DoUkar;
            lookup LeftUkar;
} blwf ;

feature cjct {  # Conjunct Forms
    script DFLT;  # Default
        language dflt;  # Default
            lookup ConjunctForms;
    script sylo;  # Syloti Nagri
        language dflt;  # Default
            lookup ConjunctForms;
} cjct ;

feature dist {  # Distances
    script DFLT;  # Default
        language dflt;  # Default
            lookup DoUkarAdj;
            lookup LeftUkarAdj;
            lookup THalfAdj;
            lookup THalfUkarAdj;
            lookup DoComma;
    script sylo;  # Syloti Nagri
        language dflt;  # Default
            lookup DoUkarAdj;
            lookup LeftUkarAdj;
            lookup THalfAdj;
            lookup THalfUkarAdj;
            lookup DoComma;
} dist ;

feature half {  # Half Forms
    script DFLT;  # Default
        language dflt;  # Default
            lookup HalfForms;
            lookup RoHalf;
            lookup RoPre;
    script sylo;  # Syloti Nagri
        language dflt;  # Default
            lookup HalfForms;
            lookup RoHalf;
            lookup RoPre;
} half ;

feature liga {  # Standard Ligatures
    script DFLT;  # Default
        language dflt;  # Default
            lookup MarkLigs;
    script sylo;  # Syloti Nagri
        language dflt;  # Default
            lookup MarkLigs;
} liga ;

feature mark {  # Mark Positioning
    script DFLT;  # Default
        language dflt;  # Default
            lookup UDias;
            lookup LDias;
    script sylo;  # Syloti Nagri
        language dflt;  # Default
            lookup UDias;
            lookup LDias;
} mark ;

feature rclt {  # Required Contextual Alternates
    script DFLT;  # Default
        language dflt;  # Default
            lookup FinalHalant;
    script sylo;  # Syloti Nagri
        language dflt;  # Default
            lookup FinalHalant;
} rclt ;
