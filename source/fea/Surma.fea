#  All mark glyphs (for GDEF table)
#  Includes word-final hasant (uniA806.final) but not regular 'circumflex' hasant (uniA806)
#  Includes subjoined t,n,r,l,s
#  Does not include marks with width (vowel signs a, i, u and o) but includes .left version of u since it has no width
@GDEF_marks = [ uniA802 uniA802A80B uniA806.final uniA80B uniA80B.left uniA814.half uniA818.half uniA81E.half uniA81F.half uniA821.half		
    uniA825.left uniA826 uniA826.left uniA826.mid uniA826A80B ] ;	

table GDEF {
GlyphClassDef
    @GDEF_bases, # base glyphs
    ,     # no ligature glyphs
    @GDEF_marks, # mark glyphs
    ;     # no component glyphs
 } GDEF;


#**********************************
#  Mark Classes
#**********************************
    markClass uniA802 <anchor -220 1160> @MARK_U ;
    markClass uniA802A80B <anchor -405 1160> @MARK_U ;
    markClass uniA80B <anchor -230 1160> @MARK_U ;
    markClass uniA826 <anchor -220 1160> @MARK_U ;
    markClass uniA826A80B <anchor -402 1160> @MARK_U ;
    markClass uniA806.final <anchor -490 -150> @MARK_L ;
#    markClass uniA806.final <anchor 0 0> @MARK_U ;	# commented out sps - not an upper mark


